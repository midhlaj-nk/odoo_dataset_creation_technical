            "source": [
                "# ðŸš€ Odoo Production Dataset Generator (Colab Self-Contained Edition)\n",
                "\n",
                "This notebook is optimized for Google Colab. It writes the generation engine directly to the environment, bypassing any GitHub sync issues.\n",
                "\n",
                "### ðŸ› ï¸ Prerequisites\n",
                "1. **Runtime**: Go to **Runtime > Change runtime type** and select **T4 GPU**.\n",
                "2. **Google Drive**: You will need to mount your drive to save the final results."
            ]
        },
        {
            "cell_type": "code",
            "execution_count": null,
            "metadata": {},
            "outputs": [],
            "source": [
                "# --- STEP 1: CONFIGURATION ---\n",
                "GROQ_API_KEY = \"your_groq_api_key_here\" # <-- PASTE YOUR KEY HERE\n",
                "\n",
                "USE_GROQ = True\n",
                "GROQ_MODEL = \"llama-3.1-8b-instant\"\n",
                "LOCAL_MODEL = \"llama3\""
            ]
        },
        {
            "cell_type": "code",
            "execution_count": null,
            "metadata": {},
            "outputs": [],
            "source": [
                "%%writefile generate_odoo_dataset.py\n",
                "import ast, os, sys, argparse, csv, logging, json, time, concurrent.futures\n",
                "from abc import ABC, abstractmethod\n",
                "from typing import List, Dict, Any, Optional, Set, Tuple\n",
                "import pandas as pd\n",
                "import requests\n",
                "from tqdm import tqdm\n",
                "from tenacity import retry, stop_after_attempt, wait_exponential, retry_if_exception_type\n",
                "\n",
                "logging.basicConfig(level=logging.INFO, format='%(asctime)s - %(levelname)s - %(message)s', handlers=[logging.StreamHandler(sys.stdout)])\n",
                "logger = logging.getLogger(__name__)\n",
                "\n",
                "class LLMProvider(ABC):\n",
                "    @abstractmethod\n",
                "    def generate_description(self, src: str) -> str: pass\n",
                "\n",
                "class OllamaProvider(LLMProvider):\n",
                "    def __init__(self, model, url=\"http://localhost:11434\"): self.model, self.url = model, f\"{url.rstrip('/')}/api/generate\"\n",
                "    @retry(stop=stop_after_attempt(3), wait=wait_exponential(1, 4, 10), retry=retry_if_exception_type(requests.exceptions.RequestException))\n",
                "    def generate_description(self, src):\n",
                "        p = f\"Analyze this Odoo code and describe business logic. ONLY text.\\n\\nCode:\\n{src}\"\n",
                "        r = requests.post(self.url, json={\"model\": self.model, \"prompt\": p, \"stream\": False}, timeout=60)\n",
                "        r.raise_for_status(); return r.json().get(\"response\", \"\").strip()\n",
                "\n",
                "class OpenAIProvider(LLMProvider):\n",
                "    def __init__(self, model, key, url=\"https://api.openai.com/v1\"): self.model, self.key, self.url = model, key, url.rstrip('/')\n",
                "    @retry(stop=stop_after_attempt(3), wait=wait_exponential(1, 4, 10), retry=retry_if_exception_type(requests.exceptions.RequestException))\n",
                "    def generate_description(self, src):\n",
                "        p = f\"Analyze this Odoo code and describe business logic. ONLY text.\\n\\nCode:\\n{src}\"\n",
                "        h = {\"Authorization\": f\"Bearer {self.key}\", \"Content-Type\": \"application/json\"}\n",
                "        r = requests.post(f\"{self.url}/chat/completions\", headers=h, json={\"model\": self.model, \"messages\": [{\"role\": \"user\", \"content\": p}], \"temperature\": 0.3}, timeout=60)\n",
                "        r.raise_for_status(); return r.json()['choices'][0]['message']['content'].strip()\n",
                "\n",
                "class OdooASTParser:\n",
                "    @staticmethod\n",
                "    def get_decorators(n):\n",
                "        decs = []\n",
                "        if not hasattr(n, 'decorator_list'): return []\n",
                "        for d in n.decorator_list:\n",
                "            if isinstance(d, ast.Name): decs.append(f\"@{d.id}\")\n",
                "            elif isinstance(d, ast.Attribute):\n",
                "                ps, c = [], d\n",
                "                while isinstance(c, ast.Attribute): ps.append(c.attr); c = c.value\n",
                "                if isinstance(c, ast.Name): ps.append(c.id)\n",
                "                decs.append(f\"@{'.'.join(reversed(ps))}\")\n",
                "            elif isinstance(d, ast.Call):\n",
                "                fn, f, args = \"\", d.func, []\n",
                "                if isinstance(f, ast.Name): fn = f.id\n",
                "                elif isinstance(f, ast.Attribute):\n",
                "                    ps, c = [], f\n",
                "                    while isinstance(c, ast.Attribute): ps.append(c.attr); c = c.value\n",
                "                    if isinstance(c, ast.Name): ps.append(c.id)\n",
                "                    fn = \".\".join(reversed(ps))\n",
                "                for a in d.args:\n",
                "                    if isinstance(a, ast.Constant): args.append(repr(a.value))\n",
                "                decs.append(f\"@{fn}({', '.join(args)})\")\n",
                "        return decs\n",
                "    @staticmethod\n",
                "    def get_sig(n, decs):\n",
                "        as_ = [a.arg for a in n.args.args]\n",
                "        if n.args.vararg: as_.append(f\"*{n.args.vararg.arg}\")\n",
                "        if n.args.kwarg: as_.append(f\"**{n.args.kwarg.arg}\")\n",
                "        s = f\"def {n.name}({', '.join(as_)}):\"\n",
                "        return \"\\n\".join(decs) + \"\\n    \" + s if decs else s\n",
                "    @staticmethod\n",
                "    def parse(fp, mn, rp):\n",
                "        res = []\n",
                "        try:\n",
                "            with open(fp, \"r\", encoding=\"utf-8\") as f: c = f.read()\n",
                "            t = ast.parse(c)\n",
                "        except: return []\n",
                "        fd, fn = os.path.dirname(rp), os.path.basename(rp)\n",
                "        for n in t.body:\n",
                "            if isinstance(n, ast.ClassDef):\n",
                "                cn, mon = n.name, \"\"\n",
                "                for i in n.body:\n",
                "                    if isinstance(i, ast.Assign):\n",
                "                        for tar in i.targets:\n",
                "                            if isinstance(tar, ast.Name):\n",
                "                                if tar.id == \"_name\" and isinstance(i.value, ast.Constant): mon = i.value.value\n",
                "                                elif tar.id == \"_inherit\" and not mon:\n",
                "                                    if isinstance(i.value, ast.Constant): mon = i.value.value\n",
                "                                    elif isinstance(i.value, ast.List) and i.value.elts:\n",
                "                                        if isinstance(i.value.elts[0], ast.Constant): mon = i.value.elts[0].value\n",
                "                for i in n.body:\n",
                "                    if isinstance(i, ast.FunctionDef):\n",
                "                        ds = (ast.get_docstring(i) or \"\").strip()\n",
                "                        try: s = ast.get_source_segment(c, i)\n",
                "                        except: s = None\n",
                "                        res.append({\"Module Name\": mn, \"File Directory\": fd, \"File Name\": fn, \"model name\": mon, \"Class name\": cn, \"Function name\": OdooASTParser.get_sig(i, OdooASTParser.get_decorators(i)), \"Description\": ds, \"Source Code\": s})\n",
                "        return res\n",
                "\n",
                "class DatasetGenerator:\n",
                "    def __init__(self, op, of, prov=None, conc=5, lim=0, th=0):\n",
                "        self.op, self.of, self.prov, self.conc, self.lim, self.th = op, of, prov, conc, lim, th\n",
                "        self.st, self.cf = time.time(), f\"{of}.checkpoint.csv\"\n",
                "        self.f=[ \"Module Name\", \"File Directory\", \"File Name\", \"model name\", \"Class name\", \"Function name\", \"Description\"]\n",
                "        self.pk = set()\n",
                "    def load(self):\n",
                "        if os.path.exists(self.cf):\n",
                "            try: \n",
                "                d=pd.read_csv(self.cf)\n",
                "                for _, r in d.iterrows(): self.pk.add((str(r['Module Name']), str(r['File Name']), str(r['Function name'])))\n",
                "            except: pass\n",
                "    def scan(self):\n",
                "        p = os.path.join(self.op, \"addons\") if os.path.exists(os.path.join(self.op, \"addons\")) else self.op\n",
                "        ts = []\n",
                "        ms = sorted([d for d in os.listdir(p) if os.path.isdir(os.path.join(p,d))])\n",
                "        for m in tqdm(ms, desc=\"Scan\"): \n",
                "            mp = os.path.join(p,m)\n",
                "            if not os.path.exists(os.path.join(mp,\"__init__.py\")) and not os.path.exists(os.path.join(mp,\"__manifest__.py\")): continue\n",
                "            for r, _, fs in os.walk(mp):\n",
                "                for f in fs:\n",
                "                    if f.endswith(\".py\"): ts.extend(OdooASTParser.parse(os.path.join(r, f), m, os.path.relpath(os.path.join(r, f), mp)))\n",
                "        return ts\n",
                "    def proc(self, r):\n",
                "        k = (str(r['Module Name']), str(r['File Name']), str(r['Function name']))\n",
                "        if k in self.pk: return None\n",
                "        if self.prov and r.get(\"Source Code\"):\n",
                "            try:\n",
                "                d = self.prov.generate_description(r[\"Source Code\"])\n",
                "                if d: r[\"Description\"] = d.replace('\\n', ' ').replace('\\r', '')\n",
                "            except: pass\n",
                "        e = {k: r[k] for k in self.f}\n",
                "        if e[\"Description\"]: e[\"Description\"] = \"\".join(c for c in str(e[\"Description\"]) if (0x20 <= ord(c) <= 0xD7FF) or c in ('\\t','\\n','\\r'))\n",
                "        return e\n",
                "    def run(self):\n",
                "        self.load(); ts = self.scan()\n",
                "        p = [t for t in ts if (str(t['Module Name']), str(t['File Name']), str(t['Function name'])) not in self.pk][:self.lim] if self.lim > 0 else [t for t in ts if (str(t['Module Name']), str(t['File Name']), str(t['Function name'])) not in self.pk]\n",
                "        h = not os.path.exists(self.cf) or os.stat(self.cf).st_size == 0\n",
                "        with open(self.cf, 'a', newline='', encoding='utf-8') as f:\n",
                "            w = csv.DictWriter(f, fieldnames=self.f)\n",
                "            if h: w.writeheader()\n",
                "            with concurrent.futures.ThreadPoolExecutor(max_workers=self.conc) as exe:\n",
                "                futs = {exe.submit(self.proc, t): t for t in p}\n",
                "                pb = tqdm(total=len(p), desc=\"AI\")\n",
                "                try:\n",
                "                    for fut in concurrent.futures.as_completed(futs):\n",
                "                        if self.th > 0 and (time.time()-self.st)/3600 >= self.th:\n",
                "                            for f2 in futs: f2.cancel()\n",
                "                            break\n",
                "                        res = fut.result()\n",
                "                        if res: w.writerow(res); f.flush(); self.pk.add((res['Module Name'], res['File Name'], res['Function name']))\n",
                "                        pb.update(1)\n",
                "                except: pass\n",
                "                finally: pb.close()\n",
                "        try:\n",
                "            df = pd.read_csv(self.cf)\n",
                "            with pd.ExcelWriter(self.of, engine='openpyxl') as wrt:\n",
                "                for m, g in df.groupby(\"Module Name\"): g.to_excel(wrt, sheet_name=\"\".join(c for c in str(m) if c not in \":\\\\/?*[]\")[:31], index=False)\n",
                "            logger.info(\"Excel Saved.\")\n",
                "        except Exception as e: logger.error(f\"Export Failed: {e}\")\n",
                "\n",
                "if __name__ == \"__main__\":\n",
                "    p = argparse.ArgumentParser()\n",
                "    p.add_argument(\"odoo\"); p.add_argument(\"out\")\n",
                "    p.add_argument(\"--provider\", choices=['ollama', 'openai'], default='ollama')\n",
                "    p.add_argument(\"--model\"); p.add_argument(\"--openai-key\"); p.add_argument(\"--openai-url\", default=\"https://api.groq.com/openai/v1\")\n",
                "    p.add_argument(\"--concurrency\", type=int, default=5); p.add_argument(\"--limit\", type=int, default=0); p.add_argument(\"--timeout\", type=float, default=0)\n",
                "    a = p.parse_args(); prov = None\n",
                "    if a.model:\n",
                "        if a.provider == 'ollama': prov = OllamaProvider(a.model)\n",
                "        else: prov = OpenAIProvider(a.model, a.openai_key, a.openai_url)\n",
                "    DatasetGenerator(a.odoo, a.out, prov, a.concurrency, a.limit, a.timeout).run()\n"
            ]
        },
